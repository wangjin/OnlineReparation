<div class="site clearfix">
	<div id="site-container" class="context-loader-container" data-pjax-container="">

		<div class="pagehead">
			<div class="container">
				<ul class="tabs">
					<li>
						<%= link_to '报修处理',{:action => 'index'}, :class => 'js-selected-navigation-item' %>
					</li>
					<li>
						<%= link_to '信息管理',{:action => 'messages'},:class => 'js-selected-navigation-item selected' %>
					</li>
					<li>
						<%= link_to '信息统计',{:action => 'informations'},:class => 'js-selected-navigation-item' %>
					</li>
					<% if session[:user].usertype.to_i == 0 %>
					<li>
						<%= link_to '材料管理',{:action => 'materials'},:class => 'js-selected-navigation-item' %>
					</li>
					<li>
						<%= link_to '维修人员管理',{:action => 'accendants'},:class => 'js-selected-navigation-item' %>
					</li>
					<% end %>
				</ul>
				<br/>
			</div>
		</div><!-- /.pagehead -->

		<div class="container">
			<div class="columns explorecols explore">
				<div class="main">
					<div id="trending-repositories" class="trending-repositories context-loader-container">

					</div>
				</div><!-- /.columns.explorecols -->
			</div>

		</div>
		<div class="modal-backdrop"></div>
	</div>
</div><!-- end of wrapper - hack for sticky footer -->

